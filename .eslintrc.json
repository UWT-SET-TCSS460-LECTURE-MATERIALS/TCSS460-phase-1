{
    "root": true,
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "script"
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "prettier"
    ],
    "env": {
        "es2021": true,
        "node": true
    },
    "rules": {
        //  "no-console": "warn",
        "@typescript-eslint/naming-convention": [
            "warn",
            {
                "selector": "variable",
                "format": ["camelCase", "UPPER_CASE"]
            },
            {
                "selector": "function",
                "format": ["camelCase"]
            }
        ],
        "@typescript-eslint/no-unused-vars": "warn"
    }
}
